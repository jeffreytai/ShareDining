<div class="kitchen-new-container">
  <h2 class="kitchen-new-title">Add Your Kitchen</h2>
  <h3 class="kitchen-new-subtitle">List your space on ShareDining and join the kitchen economy</h3>

  <!--TODO need placeholders giving examples-->
  <%= form_for @kitchen, url: {action: "create"} do |f| %>
      <fieldset class="fieldset fieldset-basic current" id="basic-information">
        <legend class="fieldset-title">Basic Information</legend>
        <ul class="input-list">
          <li>
            <label for="title">Title</label>
            <%= f.text_field :title, id: "title" %>
          </li>
          <li>
            <label for="description">Tell us briefly about your kitchen space so the renters can get a quick idea about
              the space</label>
            <%= f.text_area :description, id: "description", placeholder: "E.g. Full stocked commercial kitchen typically serving 30 cover restaurant." %>
          </li>
          <li>
            <label for="location">Location</label>
            <%= f.text_field :location, id: "location" %>
          </li>
          <li>
            <fieldset class="input-fieldset">
              <legend class="input-fieldset-title">Type of Kitchen</legend>
              <ul class="radio-selection-list clearfix">
                <li>
                  <%= f.check_box :rental_space, id: "whole-kitchen-option" %>
                  <label for="whole-kitchen-option" tabindex="0">Whole Kitchen</label>
                </li>
                <li>
                  <%= f.check_box :rental_space, id: "shared-space-option" %>
                  <label for="shared-space-option" tabindex="0">Shared Space</label>
                </li>
              </ul>
            </fieldset>
          </li>
        </ul>
      </fieldset>

