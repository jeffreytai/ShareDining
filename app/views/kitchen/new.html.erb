<div class="kitchen-new-container">
  <h2 class="kitchen-new-title">Add Your Kitchen</h2>
  <h3 class="kitchen-new-subtitle">List your space on ShareDining and join the kitchen economy</h3>

  <!--TODO need placeholders giving examples-->
  <%= form_for @kitchen, url: {action: "create"}, :html => {:multipart => true} do |f| %>
      <fieldset class="fieldset fieldset-basic current" id="basic-information">
        <legend class="fieldset-title">Basic Information</legend>
        <ul class="input-list">
          <%= f.hidden_field :user_id, value: current_user.id %>
          <li>
            <label for="title">Title</label>
            <%= f.text_field :title, id: "title", required: "true" %>
          </li>
          <li>
            <label for="description">Tell us briefly about your kitchen space so the renters can get a quick idea about
              the space</label>
            <%= f.text_area :description, id: "description", required: "true", placeholder: "E.g. Full stocked commercial kitchen typically serving 30 cover restaurant." %>
          </li>
          <li>
            <label for="location">Location</label>
            <%= f.text_field :location, id: "location", required: "true" %>
            <script>
              // TODO: this should be moved to a js file and attached to window
              function initialize() {
                var options = {
                  componentRestrictions: { country: "GB" }
                }
                var input = document.getElementById('location');
                var autocomplete = new google.maps.places.Autocomplete(input, options);
              }
            </script>
            <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBDcgmjQMh77M_6_u3k0z1ymYgeEIrENHM&libraries=places&callback=initialize" async defer></script>
          </li>
          <li>
            <fieldset class="input-fieldset">
              <legend class="input-fieldset-title">Type of Kitchen</legend>
              <ul class="radio-selection-list clearfix">
                <li>
                  <%= f.check_box :rental_space, id: "whole-kitchen-option", name: "whole_kitchen" %>
                  <label for="whole-kitchen-option" tabindex="0">Whole Kitchen</label>
                </li>
                <li>
                  <%= f.check_box :rental_space, id: "shared-space-option", name: "shared_space" %>
                  <label for="shared-space-option" tabindex="0">Shared Space</label>
                </li>
              </ul>
            </fieldset>
          </li>
        </ul>
      </fieldset>

