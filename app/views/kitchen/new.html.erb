            <li class="checkbox-fieldset-list-item"><%= check_box_tag 'kitchen[other_amenities][]', "Wheelchair accessible", @kitchen.other_amenities.include?("Wheelchair accessible"), id: "wheelchair-accessible", class: "inventory-fieldlist-checkbox" %>
              <label for="wheelchair-accessible">Wheelchair accessible</label>
              <input type="number" value="1" aria-label="Wheelchair accessible Quantity" step="1" min="1" class="inventory-quantity-input">
            </li>
          </ul>
        </fieldset>
      </fieldset>
      <fieldset class="fieldset" id="additional-information">
        <legend class="fieldset-title">Additional Information</legend>
        <ul class="input-list">
          <li>
            <label for="rules">Kitchen Rules or Instructions</label>
            <%= f.text_area :kitchen_rules_and_instructions, id: "rules", placeholder: "- E.g. Details about location of operational switches.
- Instructions to bring specific equipment.
- Any available storage space.
- Details about deliveries." %>
          </li>
          <li>
            <label for="price">Tell us how much you would like to rent the kitchen space for</label>
            <span class="price-currency-symbol">£</span>
            <%= f.text_field :price, type: "number", id: "price", class: "price-input", required: "true", min: "10" %>
            <span class=price-description>/hour</span>
          </li>
          <li>
            <label for="additional-details">Let us know about any other details you think the renter might want to know
              about</label>
            <%= f.text_area :additional_details, id: "additional-details", placeholder: "E.g Highly recommend local supplier [x]." %>
          </li>
          <li>
            <label for="cancellation_policy">Cancellation Policy</label>
            <%= f.radio_button :cancellation_policy, 'flexible', checked: true %><%= f.label :cancellation_policy, "Flexible", style: "display: inline;" %>
            <br/>
            2 week in advance: 75% refund<br/>
            1 week in advance: 50% refund<br/>
            48 hours in advance: 25% refund<br/>
            Less than 24 hours: 0% refund<br/>

            <%= f.radio_button :cancellation_policy, 'limited' %><%= f.label :cancellation_policy, "Limited", style: "display: inline;" %>
            <br/>
            2 weeks in advance: 100% refund<br/>
            1 week in advance: 50% refund<br/>
            Less than 1 week: 0% refund<br/>

            <%= f.radio_button :cancellation_policy, 'strict' %><%= f.label :cancellation_policy, "Strict", style: "display: inline;" %>
            <br/>
            No refund on cancellation.
          </li>
        </ul>
      </fieldset>
      <!-- TODO: add availability section -->

      <fieldset class="fieldset" id="pictures">
        <legend class="fieldset-title">Availability</legend>
        <%# form_for @availability, url: {action: "create"} do |avail| %>
            <ul class="day-availability-list">
              <li class="day-availability-list-element">
                <h2>Sunday</h2>
                <ul class="date-slider" data-name="sunday_availability"></ul>
              </li>
              <li class="day-availability-list-element">
                <h2>Monday</h2>
                <ul class="date-slider" data-name="monday_availability"></ul>
              </li>
              <li class="day-availability-list-element">
                <h2>Tuesday</h2>
                <ul class="date-slider" data-name="tuesday_availability"></ul>
              </li>
              <li class="day-availability-list-element">
                <h2>Wednesday</h2>
                <ul class="date-slider" data-name="wednesday_availability"></ul>
              </li>
              <li class="day-availability-list-element">
                <h2>Thursday</h2>
                <ul class="date-slider" data-name="thursday_availability"></ul>
              </li>
              <li class="day-availability-list-element">
                <h2>Friday</h2>
                <ul class="date-slider" data-name="friday_availability"></ul>
              </li>
              <li class="day-availability-list-element">
                <h2>Saturday</h2>
                <ul class="date-slider" data-name="saturday_availability"></ul>
              </li>
            </ul>
        <%# end %>

        <br/>
        <legend class="fieldset-title">Upload Pictures</legend>
        <%= f.file_field :photos, multiple: true %>
        <!--TODO this needs to be changed to actual picture input-->
      </fieldset>
  <% end %>

  <div class="kitchen-new-button-container clearfix">
    <button class="prev-button transparent-button" disabled>Previous</button>
    <button class="next-button transparent-button">Next</button>
  </div>

</div>
</body>
