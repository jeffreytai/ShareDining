              <input type="number" value="1" aria-label="Washing up sink (Single compartment) Quantity" step="1" min="1" class="inventory-quantity-input">
            </li>
            <li class="checkbox-fieldset-list-item"><%= f.check_box :washing_station, id: "washing-up-sink-double-compartment", class: "inventory-fieldlist-checkbox" %>
