<%= render 'shared/icons' %>
<header class="main-nav" role="banner">
  <a href="<%= root_url %>"><h1 class="main-logo">ShareDining</h1></a>
  <nav role="navigation">
    <ul class="landing-links">
      <% if user_signed_in? %>
          <li><%= link_to "Messages", root_url %></li>
          <li><%= link_to "Help", root_url %></li>
          <li><%= link_to "Account", root_url %></li>
          <li><%= link_to "Sign out", destroy_user_session_path %></li>
      <% else %>
          <li><%= link_to "About", root_url %></li>
          <li><a href="#" class="nav-login-button">Login</a></li>
          <li><a href="#" class="nav-signup-button">Sign Up</a></li>
      <% end %>
    </ul>
    <button class="transparent-button landing-rent-button">
      Rent Your Kitchen
    </button>
  </nav>
</header>
